n = int(input("enter number of rows"))
List1 = []
for i in range(n):
    List2 = []
    for j in range(i+1):
        if j == 0 or j == i:
            List2.append(1)
        else:
            List2.append(List1[i-1][j-1] + List1[i-1][j])
    List1.append(List2)

for i in range(n):
    for j in range(n-1-i):
        print(format(" ", "<2"), end="")
    for j in range(i+1):
        print(format(List1[i][j], "<3"), end=" ")
    print()
